<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Login\LoginBundle\Entity\Student" table="student">
    <id name="indnum" type="integer" column="indNum">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="studentid" type="string" column="studentID" length="10" nullable="false"/>
    <field name="firstname" type="string" column="firstname" length="30" nullable="false"/>
    <field name="lastname" type="string" column="lastname" length="30" nullable="false"/>
    <field name="gender" type="string" column="gender" length="20" nullable="false"/>
    <field name="deptName" type="string" column="dept_Name" length="30" nullable="false"/>
    <field name="year" type="string" column="year" length="10" nullable="false"/>
  </entity>
</doctrine-mapping>
