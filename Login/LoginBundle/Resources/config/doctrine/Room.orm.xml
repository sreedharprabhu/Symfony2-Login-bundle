<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Login\LoginBundle\Entity\Room" table="room">
    <id name="indnum" type="integer" column="indNum">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hallname" type="string" column="hallname" length="50" nullable="false"/>
    <field name="roomno" type="decimal" column="roomNo" nullable="false"/>
    <field name="type" type="string" column="type" length="8" nullable="true"/>
    <field name="monthlycost" type="decimal" column="monthlyCost" nullable="true"/>
  </entity>
</doctrine-mapping>
